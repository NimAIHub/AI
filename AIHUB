		page.Name = "page"
		page.Parent = archives
		page.Active = true
		page.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		page.BackgroundTransparency = 1.000
		page.BorderSizePixel = 0
		page.Position = UDim2.new(0, 0, 0.032212887, 0)
